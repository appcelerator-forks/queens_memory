// customDate : 'whatever',
		// customImage : currentData['userPhoto'],
		// customImageSize : scannedItem.size,
		// photoFullName : $.soundPeopleNameTA.value.split(" "),
		
function replaceChar(str) {
	str = str.replace('&', '&amp;');
	str = str.replace('<', '&lt;');
	str = str.replace('>', '&gt;');
}

function replacer(str) {
	if (str.includes("&" || ">" || "<")) {
		replaceChar(str);
		// str=str.replace('&', '&amp;');
		// str=str.replace('<', '&lt;');
		// str=str.replace('>', '&gt;');

	} else {
		return str;
	}
}

