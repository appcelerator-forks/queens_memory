function replaceChar(str){
	str=str.replace('&', '&amp;');
	str=str.replace('<', '&lt;');
	str=str.replace('>', '&gt;');
}

function replacer(str){
	if (str.includes("&" || ">" || "<")){
		replaceChar(str);
		// str=str.replace('&', '&amp;');
		// str=str.replace('<', '&lt;');
		// str=str.replace('>', '&gt;');
		
	} else {
		return str;
	}
}

// function replaceChar(str){
	// str=str.replace('&', '&amp;');
	// str=str.replace('<', '&lt;');
	// str=str.replace('>', '&gt;');
// }

function replacer(i, string){
	if (i.includes('&' || '>' || '<')){
		// replaceChar(str);
		console.log("There are illegal chracters in here!");
		i=i.replace('&', 'NOT AMPERSAND!!!!!');
		// str=str.replace('&', '&amp;');
		i=i.replace('<', '&lt;');
		i=i.replace('>', '&gt;');
		
	} else {
		return string;
	}
}

